<!DOCTYPE html>

<html>
  <head>
		<!-- Required meta tags -->
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

		<!-- CSS -->
        {% block stylesheets %}

            <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
            {{ encore_entry_link_tags('app') }}
            {{ encore_entry_link_tags('main') }}
            <link 
                href="https://fonts.googleapis.com/css?family=Montserrat:300,400,500,600&display=swap" 
                rel="stylesheet"/>

        {% endblock %}

		<title>{% block title %}Personnes recherchées{% endblock %}</title>
  </head>

    {# {% block body %} #}
    
        <body {# style = "position: absolute;" #} class = "">
            <!-- Navbar -->
            <nav class="navbar navbar-expand-lg bg-dark">
                <a class="navbar-brand" href="#">
                    <img src="{{asset('build/images/logo.svg')}}" alt="logo">
                </a>
                <button class="navbar-toggler" 
                    type="button" 
                    data-toggle="collapse" 
                    data-target="#navbarSupportedContent" 
                    aria-controls="navbarSupportedContent" 
                    aria-expanded="false" 
                    aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
            
                <div class="collapse navbar-collapse" id="navbarSupportedContent">
                    <ul class="navbar-nav ml-auto">
                        <li class="nav-item">
                            <a class="nav-link" href="#">
                                Services
                            </a>
                        </li>
                        {# <li class="nav-item">
                            <a class="nav-link" href="#">
                                Rechercher un document
                            </a>
                        </li> #}
                    </ul>
                    <div class="actions-menu">
                        <a type="button" 
                            class="btn btn-secondary" 
                            id="signin-button"
                            href = "{{ path('app_login') }}">
                            Se connecter
                        </a>
                        <a type="button" 
                            class="btn btn-primary btn-orange" 
                            id="signup-button"
                            href = "#">
                            Créer un compte
                        </a>
                        <button type="button" 
                            class="btn btn-link">
                            FR
                        </button>
                    </div>
                </div>
            </nav>
            {% block container %}
            {% endblock %}

            <!-- Footer -->
            <div class="footer" {# style = "position: fixed;" #}>
                <div class="container">
                    <div class="information-block">
                        <div class="row">
                            <div class="box col-12 col-md-3 col-lg-3 public-services">
                                <p class="title">Services publics</p>
                                <div>
                                    <img src="{{asset('build/images/i_bg_white.svg')}}" alt="">
                                    <a href="#">
                                        A Propos
                                    </a>		
                                </div>
                                <div>
                                    <img src="{{asset('build/images/phone.svg')}}" alt="">
                                    <a href="#">
                                        Nous contacter
                                    </a>		
                                </div>
                            </div>
                            <div class="box col-12 col-md-3 col-lg-5 contribuate">
                                <p class="title">Contribuer</p>
                                <div>
                                    <img src="{{asset('build/images/mail.svg')}}" alt="">
                                    <a href="#">
                                        Faire une suggestion
                                    </a>		
                                </div>
                                <div>
                                    <img src="{{asset('build/images/pencil.svg')}}" alt="">
                                    <a href="#">
                                        Signaler un problème
                                    </a>	
                                </div>
                            </div>
                            <div class="box col-12 col-md-6 col-lg-4 social-media">
                                <p class="title">Suivez-nous sur</p>
                                <div>
                                    <img src="{{asset('build/images/facebook.svg')}}">
                                    <img src="{{asset('build/images/twitter.svg')}}">
                                    <img src="{{asset('build/images/youtube.svg')}}">
                                </div>
                            </div>
                        </div>
                        <div class="copyright">
                            <p class="text-center">© 2019 Portail de l'administration béninoise - service-public.bj</p>
                        </div>
                    </div>
                </div>
            </div>
    {# {% endblock %} #}
    
    {% block javascripts %}
    
        <!-- Optional JavaScript -->
	    <!-- jQuery first, then Popper.js, then Bootstrap JS -->

        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>

		<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js" 
			integrity="sha384-ZMP7rVo3mIykV+2+9J3UJ46jBk0WLaUAdn689aCwoqbBJiSnjAK/l8WvCWPIPm49" 
			crossorigin="anonymous">
		</script>
		<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js" 
			integrity="sha384-ChfqqxuZUCnJSK3+MXmPNIyE6ZbWh2IMqE241rYiqJxyMiZ6OW/JmZQ5stwEULTy" 
			crossorigin="anonymous">
		</script>

        {# <script src="{{ asset('bundles/fosjsrouting/js/router.min.js') }}"></script>
        <script src="{{ path('fos_js_routing_js', { callback: 'fos.Router.setData' }) }}"></script> #}

        {{ encore_entry_script_tags('app') }}

        <script>
            var delete_icon = "{{asset('build/images/delete_icon.png')}}"
            var edit_icon = "{{asset('build/images/edit_icon.png')}}"
        </script>

    {% endblock %}

</html>

